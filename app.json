{
  "expo": {
    "name": "Rise-And-Ride",
    "slug": "Rise-And-Ride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_config/cute-robot-icon.jpg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/app_config/LoadingScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFE450"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_config/LoadingScreen.png",
        "backgroundColor": "#FFE450"
      }
    },
    "web": {
      "favicon": "./src/assets/app_config/favicon.png"
    }
  }
}
